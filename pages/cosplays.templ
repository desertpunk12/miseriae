templ CosplaysHead() {
	<script id="tailwind-config">
        tailwind.config = {
            darkMode: "class",
            theme: {
                extend: {
                    colors: {
                        // Updated for a brighter, more saturated "Bubblegum Pop" pink palette
                        "primary": "#ff69b4", // Hot Pink
                        "primary-light": "#ffe0f0", // Lighter Hot Pink
                        "primary-dark": "#e03b8c", // Slightly darker hot pink for hover states
                        "background-light": "#fffafb", // Very light pinkish white
                        "background-dark": "#2c1523", // Deeper dark pink
                        "accent-pink": "#f4a2d4", // A secondary playful pink
                        "text-dark": "#3c1e30", // Dark text to contrast with pinks
                    },
                    fontFamily: {
                        "display": ["Spline Sans", "sans-serif"],
                        "body": ["Noto Sans", "sans-serif"]
                    },
                    borderRadius: {
                        "DEFAULT": "1rem",
                        "lg": "1.5rem",
                        "xl": "2rem",
                        "2xl": "2.5rem",
                        "3xl": "3rem","full": "9999px"
                    },
                    backgroundImage: {
                        // Sparkles pattern updated to reflect the new primary pink
                        'sparkles': "url(\"data:image/svg+xml,%3Csvg width='20' height='20' viewBox='0 0 20 20' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M10 0L12.5 7.5L20 10L12.5 12.5L10 20L7.5 12.5L0 10L7.5 7.5L10 0Z' fill='%23ff69b4' fill-opacity='0.2'/%3E%3C/svg%3E\")",
                        'gradient-pop': 'linear-gradient(135deg, var(--tw-gradient-stops))', // Custom gradient for buttons
                    }
                },
            },
        }
    </script>
	<style>::-webkit-scrollbar {
            width: 8px;
        }
        ::-webkit-scrollbar-track {
            background: #fffafb;}
        ::-webkit-scrollbar-thumb {
            background: #ff69b4;border-radius: 4px;
        }
        ::-webkit-scrollbar-thumb:hover {
            background: #e03b8c;}
        .masonry-grid {
            column-count: 1;
            column-gap: 1.5rem;
        }
        @media (min-width: 640px) {
            .masonry-grid { column-count: 2; }
        }
        @media (min-width: 1024px) {
            .masonry-grid { column-count: 3; }
        }
        .card-hover-reveal {
            transition: opacity 0.3s ease, transform 0.3s ease;
        }@keyframes float {
            0% { transform: translateY(0px); }
            50% { transform: translateY(-10px); }
            100% { transform: translateY(0px); }
        }
        .animate-float {
            animation: float 4s ease-in-out infinite;
        }
        .animate-float-delayed {
            animation: float 5s ease-in-out infinite 2s;
        }.card-transform-1 {
            transform: rotate(2deg) translateX(5px);
        }
        .card-transform-2 {
            transform: rotate(-3deg) translateX(-10px);
        }
        .card-transform-3 {
            transform: rotate(1deg) translateY(7px);
        }
        .card-transform-4 {
            transform: rotate(-1deg) translateX(3px);
        }
        .card-transform-5 {
            transform: rotate(4deg) translateY(-8px);
        }
        .card-transform-6 {
            transform: rotate(-2deg) translateX(-5px);
        }
        .card-transform-7 {
            transform: rotate(3deg) translateY(10px);
        }
        .card-transform-8 {
            transform: rotate(-4deg) translateX(15px);
        }.masonry-grid > div:not(:first-child) {
            margin-top: -30px;}
    </style>
}

